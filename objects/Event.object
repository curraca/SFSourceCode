    <validationRules>
        <fullName>Solution_Architect_Information_Custom</fullName>
        <active>true</active>
        <description>A &quot;Solution Architect Information&quot; with the value Custom , needs additional comments in the &quot;Demo Type-Others&quot;</description>
        <errorConditionFormula>OR(AND( ISNEW() , NOT(INCLUDES( Solution_Architect_Information__c , &quot;Custom&quot;)), NOT(ISBLANK( Demo_Type_Others__c ))) , AND(ISCHANGED(Solution_Architect_Information__c ), NOT(INCLUDES( Solution_Architect_Information__c , &quot;Custom&quot;)), NOT(ISBLANK( Demo_Type_Others__c ))), AND( ISNEW() , INCLUDES( Solution_Architect_Information__c , &quot;Custom&quot;), ISBLANK( Demo_Type_Others__c )),AND( ISCHANGED(Solution_Architect_Information__c) , INCLUDES( Solution_Architect_Information__c , &quot;Custom&quot;), ISBLANK( Demo_Type_Others__c )),AND(ISCHANGED(Demo_Type_Others__c ), NOT(INCLUDES( Solution_Architect_Information__c , &quot;Custom&quot;)),NOT(ISBLANK( Demo_Type_Others__c ))),AND( ISCHANGED(Demo_Type_Others__c) , INCLUDES( Solution_Architect_Information__c , &quot;Custom&quot;), ISBLANK( Demo_Type_Others__c )))</errorConditionFormula>
        <errorMessage>Please provide additional information in &quot;Demo Type-Others&quot; when Custom is selected in &quot;Solution Architect Information&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_App_Int_Tech_Tosca</fullName>
        <active>true</active>
        <description>If a Multi picklist has a value of ‘Others’ and Text Field &quot;Others App/Int/Tech/Tosca&quot; is empty then an error message is sent.</description>
        <errorConditionFormula>OR(AND( INCLUDES( Applications__c ,&quot;Others&quot;) , ISBLANK( Others_Applications__c )),AND( INCLUDES( Integrations__c , &quot;Others&quot;) , ISBLANK( Others_Integrations__c ) ),AND( INCLUDES( Technology__c ,&quot;Others&quot;), ISBLANK( Others_Technologies__c )),AND( INCLUDES( Tosca_Products__c ,&quot;Others&quot;),ISBLANK( Others_ToscaProducts__c ) ))</errorConditionFormula>
        <errorMessage>“Please specify ‘Others’. Indicate the App/Tech/Integration/Toscaproduct missing in the picklist”</errorMessage>
    </validationRules>
